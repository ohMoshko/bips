{
  "name": "@bips/server",
  "version": "0.0.1",
  "description": "Server SDK for Bips Protocol - Express middleware",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@bips/core": "workspace:*",
    "x402-express": "^1.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/supertest": "^6.0.0",
    "express": "^4.21.0",
    "supertest": "^7.0.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
