{
  "name": "@bips/react",
  "version": "0.0.1",
  "description": "React SDK for Bips Protocol",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@bips/core": "workspace:*",
    "@x402/fetch": "^2.1.0",
    "@x402/evm": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.0",
    "jsdom": "^25.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "@dynamic-labs/sdk-react-core": ">=3.0.0",
    "react": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
